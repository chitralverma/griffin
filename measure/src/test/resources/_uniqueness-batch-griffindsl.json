{
  "name": "unique_batch",
  "data.sources": [
    {
      "name": "source",
      "type": "file",
      "config": {
        "format": "avro",
        "paths": [
          "src/test/resources/users_info_src.avro"
        ]
      }
    },
    {
      "name": "target",
      "type": "file",
      "config": {
        "format": "avro",
        "paths": [
          "src/test/resources/users_info_src.avro"
        ]
      }
    }
  ],
  "evaluate.rule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "uniqueness",
        "out.dataframe.name": "dup",
        "rule": "user_id",
        "details": {
          "source": "source",
          "target": "target",
          "total": "total",
          "unique": "unique",
          "dup": "dup",
          "num": "num"
        },
        "out": [
          {
            "type": "metric",
            "name": "unique"
          },
          {
            "type": "record",
            "name": "dupRecords"
          }
        ]
      }
    ]
  },
  "sinks": [
    "CONSOLESink",
    "customSink"
  ]
}