{
  "spark": {
    "log.level": "error",
    "config": {
      "spark.master": "local[*]",
      "spark.sql.shuffle.partitions": "4",
      "spark.default.parallelism": "4",
      "spark.app.name": "GriffinStreamingApplication"
    }
  },
  "sinks": [
    {
      "name": "consoleSink",
      "type": "console",
      "isStreaming": true,
      "config": {
        "numRows": 20,
        "streamingOutputMode": "Update",
        "triggerMs": 10000,
        "options": {
          "truncate": false,
          "checkpointLocation": "/tmp/griffin/chkpnt/"
        }
      }
    }
  ]
}